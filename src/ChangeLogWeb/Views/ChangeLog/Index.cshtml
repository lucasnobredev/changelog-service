@model IList<ChangeLogWeb.Models.ViewModels.PullRequestEventViewModel>

@{
    ViewData["Title"] = "Index";
}


<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
<div class="row">
    <div class="container-fluid">
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <a class="navbar-brand" href="#">Changelog</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item active">
                        <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
                    </li>
                    
                </ul>
            </div>
        </nav>
        <div class="col-lg-12">
            @foreach (var pullrequest in Model)
            {
                <div class="card" style="width:40%; margin:2% 32%; border:0">
                    <div class="card-body">
                        <h4 class="card-title">@pullrequest.Title </h4>
                        <h6 class="card-subtitle mb-2 text-muted">@pullrequest.MergedAt by @pullrequest.MergedBy </h6>
                        <span style="background:blue; border-radius:30px; width:100%; padding:0% 3%; color:#FFF; font-size:14px">Fix</span>
                        <p class="card-text">@Html.Raw(pullrequest.Body)</p>
                        <a href="#" class="card-link">Card link</a>
                        <a href="#" class="card-link">Another link</a>
                    </div>
                </div>
            }
        </div>
    </div>
</div>