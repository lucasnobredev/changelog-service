@model IList<ChangeLogWeb.Models.ViewModels.PullRequestEventViewModel>

@{
    ViewData["Title"] = "Index";
}


<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    
@foreach (var pullrequest in Model)
{
    <div class="card" style="width:40%; margin:0 30%">
        <div class="card-body">
            <h3>(@pullrequest.RepositoryName)</h3>
            <h5 class="card-title">@pullrequest.Title</h5>
            <h6 class="card-subtitle mb-2 text-muted">Merge at @pullrequest.MergedAt by @pullrequest.MergedBy </h6>
            <p class="card-text">@Html.Raw(pullrequest.Body)</p>
            <a href="#" class="card-link">Card link</a>
            <a href="#" class="card-link">Another link</a>
        </div>
    </div>
}
